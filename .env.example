# OpenAI API Key (required for AI analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Vnstock API Key (optional - for higher rate limits and premium features)
# Get your API key at vnstocks.com
VNSTOCK_API_KEY=your_vnstock_api_key_here

# Optional: Streamlit configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0

# Optional: PandasAI configuration
PANDASAI_API_KEY=your_pandasai_api_key_if_different

# Optional: OpenAI Model Configuration
OPENAI_MODEL=gpt-4o-mini

# Optional: Logging level
LOG_LEVEL=INFO
